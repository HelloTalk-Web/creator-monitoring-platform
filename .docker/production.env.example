# 生产环境配置模板
NODE_ENV=production
LOG_LEVEL=warn

# 远程数据库连接配置
POSTGRES_DB=creator_monitoring
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_remote_password_here

# 远程数据库连接字符串
REMOTE_DATABASE_URL=postgresql://postgres:your_remote_password_here@117.72.221.238:5432/creator_monitoring

# 本地数据库连接字符串（备用）
LOCAL_DATABASE_URL=postgresql://postgres:your_remote_password_here@localhost:5433/creator_monitoring

# 数据库选择（remote/local）
